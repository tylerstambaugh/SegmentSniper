{
    
  "platform": {
    "apiRuntime": "node:20"
  },
  
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/images/*", "/favicon.ico"]
  },

  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://as-segmentsniper-api-eastus-dev.azurewebsites.net/api/:path*"
    },
    {
      "route": "/graphql",
      "rewrite": "https://as-segmentsniper-api-eastus-dev.azurewebsites.net/graphql"
    }
  ]
}
