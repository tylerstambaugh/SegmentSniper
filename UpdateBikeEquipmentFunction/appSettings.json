{
  "AzureKeyVault": {
    "BaseUrl": "https://kv-segmentsiper-dev.vault.azure.net/"
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "",
          "sinkOptions": {
            "tableName": "SegmentSniperLog",
            "autoCreateSqlTable": false
          }
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "SegmentSniper"
    }
  }
}